@model IEnumerable<AoA_Arquitetura.Models.Projeto>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link rel="stylesheet" href="~/Content/Site.css" />
</head>
<body>
    <div class="header">
        <figure><img src="~/Imagens/logo1.png" /></figure>
        <div class="header-right">
            <a class="active" href="#home">Home</a>
            <a href="#contact">Projetos</a>
            <a href="#about">Contato</a>
            <a href="#sobre">Sobre</a>
        </div>
    </div>


    <div class="row">
        <div class="column">
            <div class="content">
                <img src="~/Imagens/casa1.png" alt="Mountains" style="width:100%">

                <h3>@Html.DisplayNameFor(model => model.Nome)</h3>

                <p> @Html.DisplayNameFor(model => model.Descricao)</p>
            </div>
        </div>
        <div class="column">
            <div class="content">
                <img src="~/Imagens/casa2.png" alt="Lights" style="width:100%">
                <h3>@Html.DisplayNameFor(model => model.Nome)</h3>
 
                <p> @Html.DisplayNameFor(model => model.Descricao)</p>
            </div>
        </div>
        <div class="column">
            <div class="content">
                <img src="~/Imagens/carro2.png" alt="Nature" style="width:100%">
                <h3>@Html.DisplayNameFor(model => model.Nome)</h3>

                <p> @Html.DisplayNameFor(model => model.Descricao)</p>
            </div>
        </div>
        <div class="column">
            <div class="content">
                <img src="~/Imagens/carro2.png" alt="Mountains" style="width:100%">
                <h3>@Html.DisplayNameFor(model => model.Nome)</h3>

                <p> @Html.DisplayNameFor(model => model.Descricao)</p>
            </div>
        </div>
    </div>


      
</body>
</html>
